<?xml version="1.0"?>
<config> 
	<api>
		<resources>
			<bundle_desconect translate="title">
				<model>millenniummgt_bundle/desconect_api</model>
				<title>bundledesconect</title>                
				<acl>bundle_data</acl>
				<methods>
					<options translate="title" module="millenniummgt_bundle">
						<title>Desconectar Opcoes</title>
						<method>desconectoptions</method>
					</options>
				</methods>
			</bundle_desconect>
			<bundle_conect translate="title">
				<model>millenniummgt_bundle/conect_api</model>
				<title>bundleconect</title>                
				<acl>bundle_data</acl>
				<methods>
					<bundle translate="title" module="millenniummgt_bundle">
						<title>Conectar Opcoes</title>
						<method>conectoptions</method>
					</bundle>
				</methods>
			</bundle_conect>			
		</resources>
		<v2>
			<resources_function_prefix>
				<bundle_desconect>objectmodeldelete</bundle_desconect>
				<bundle_conect>objectmodelcreate</bundle_conect>
			</resources_function_prefix>
		</v2>
		<acl>
			<resources>
				<bundle_data translate="title" module="millenniummgt_bundle">
					<title>bundle data</title>
					<sort_order>3</sort_order>               
				</bundle_data>			
			</resources>
		</acl>
	</api>
</config>